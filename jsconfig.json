{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "strict": true,
        "jsx": "preserve",
        "importHelpers": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "baseUrl": ".",
      "paths": {
        "@/*": [
            "src/*"
        ],
        "src/*": [
          "src/*"
        ],
        "app/*": [
          "*"
        ],
        "components/*": [
          "src/components/*"
        ],
        "layouts/*": [
          "src/layouts/*"
        ],
        "pages/*": [
          "src/pages/*"
        ],
        "assets/*": [
          "src/assets/*"
        ],
        "boot/*": [
          "src/boot/*"
        ],
        "stores/*": [
          "src/stores/*"
        ],
        "vue$": [
          "node_modules/vue/dist/vue.runtime.esm-bundler.js"
        ]
      }
    },
    "lib": [
        "esnext",
        "dom",
        "dom.iterable",
        "scripthost"
    ],
    "exclude": [
      "dist",
      ".quasar",
      "node_modules"
    ]
  }